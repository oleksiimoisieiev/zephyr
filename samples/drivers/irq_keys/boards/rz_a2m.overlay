 / {
	 keyboard {
		 compatible = "irq-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&keyboard_pins>;

		 sw3 {
			 interrupt-parent = <&intc>;
			 interrupts = <R7S9210_IRQ0 IRQ_TYPE_EDGE_RISING>;
		 };
	 };
 };
